<?xml version="1.0" encoding="utf-8"?>
<program xmlns="http://www.kinoma.com/kpr/1">
    
    <skin id="color1" color="#495241" />
    <skin id="color2" color="#B8AE9C" />
    <skin id="color3" color="#8A0917" />
    <skin id="color4" color="#ACCFCC" />
    <skin id="white" color="white" />
    
    <style id="title" font="bold 18px" color="white" align="left,top"/>
    <style id="sensor" font="18px" color="black" align="left,top"/>
    
    
    <container id="body" left="0" top="0" right="0" bottom="0" skin="color1" active="true">		
		<label left="0" right="0" top="0" bottom="0" style="title" string="$.text"/>
	</container>
	

	<container id="sensorList" left="15" top="45" width="290" height="180" skin="white">
		<label left="0" right="0" top="0" bottom="0" style="sensor" string="$.text"/>
	</container>
    
    <script>
        <![CDATA[
        	application.add( new body({text:"List of Lights On"}) );
        	application.add( new sensorList({text:"Test Sensor 1"}));
        ]]>
    </script>
</program>
